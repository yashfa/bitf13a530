<body  onload="functt()">
    <script>

       function functt() {


       var final = "";
       var xmlhttp2;
       if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
           xmlhttp2 = new XMLHttpRequest();
       }
       xmlhttp2.open("GET", "../Home/hottrips2?textName=" + final, true);
       xmlhttp2.send();

       xmlhttp2.onreadystatechange = function () {
           if (xmlhttp2.readyState == 4 && xmlhttp2.status == 200) {

               response1 = xmlhttp2.responseText;

           }
           document.getElementById("tdiv1").innerHTML = response1;

       }

   }

        function func() {

            var a = document.getElementById("price").value;
            var b = document.getElementById("days").value;
            var c = document.getElementById("nights").value;
            var d = document.getElementById("attractions").value;

            var final = a + "," + b + "," + c + "," + d;

            var xmlhttp2;
            if (window.XMLHttpRequest) {// code for IE7+, Firefox, Chrome, Opera, Safari
                xmlhttp2 = new XMLHttpRequest();
            }
            xmlhttp2.open("GET", "../Home/trippkg?textName=" + final, true);
            xmlhttp2.send();

            xmlhttp2.onreadystatechange = function () {
                if (xmlhttp2.readyState == 4 && xmlhttp2.status == 200) {

                    response1 = xmlhttp2.responseText;
                 }
                document.getElementById("tdiv1").innerHTML = response1;

            }

        }
    </script>


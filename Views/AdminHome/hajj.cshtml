@Html.Partial("_header");
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Hajj Umrah Check
            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-header">
                        <h3 class="box-title">Flight Requests</h3>
                    </div>
                    <div class="box-body">
                        <table id="example2" class="table table-bordered table-hover">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Address</th>
                                    <th>City</th>
                                    <th>Country</th>
                                    <th>Phone# Res</th>
                                    <th>Cell</th>
                                    <th>Email</th>
                                    <th>Action</th>
                                </tr>       
                                @{
                                    foreach (var i in Model)
                                    {
                                        <tr>
                                            <td>@i.name</td>
                                            <td>@i.address</td>
                                            <td>@i.city</td>
                                            <td>@i.country</td>
                                            <td>@i.phone</td>
                                            <td>@i.cell_no</td>
                                            <td>@i.email</td>
                                            <td><a href="/AdminHome/remove2/@i.Id"><button class="btn btn-danger">Remove</button></a><a href="/AdminHome/detail/@i.Id"><button class="btn btn-primary">Detail</button></a></td>
                                        </tr>
                                    }

                                }
                            </thead>
                            <tbody></tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
    @Html.Partial("_footer");
    <script>
        document.getElementById("lh").className = "active";
    </script>
</html>
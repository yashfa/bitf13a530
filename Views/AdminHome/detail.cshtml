@Html.Partial("_header");
<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Hajj Umrah Customer Detailed Form
            <small></small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Level</a></li>
            <li class="active">Here</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
        <div class="row">
            <div class="col-xs-12">
                <div class="box">
                    <div class="box-header">
                        <h3 >Customer Detail</h3>
                    </div>
                    <div class="box-body">
                        <table style="width:80%">
                            <tr>
                                <th>Name:</th>
                                <td>@Model.name</td>
                            </tr>
                            <tr>
                                <th>Email:</th>
                                <td>@Model.email</td>
                            </tr>
                            <tr>
                                <th>Address:</th>
                                <td>@Model.address</td>
                            </tr>
                            <tr>
                                <th>City:</th>
                                <td>@Model.city</td>
                            </tr>
                            <tr>
                                <th>Country:</th>
                                <td>@Model.country</td>
                            </tr>
                            <tr>
                                <th>Type:</th>
                                <td>@Model.type</td>
                            </tr>
                            <tr>
                                <th>Phone:</th>
                                <td>@Model.phone</td>
                            </tr>
                            <tr>
                                <th>Cell:</th>
                                <td>@Model.cell_no</td>
                            <tr>
                                <th>Best time to contact:</th>
                                <td>@Model.best</td>
                            </tr>
                            <tr>
                                <th>People: </th>
                                <td>@Model.people</td>
                            </tr>
                            <tr>
                                <th>Adults:</th>
                                <td>@Model.adults</td>
                            <tr>
                                <th>Kids:</th>
                                <td>@Model.kids</td>
                            <tr>
                                <th>Print Name</th>
                                <td>@Model.print__name</td>
                            <tr>
                                <th>Date:</th>
                                <td>@Model.date</td>
                            <tr>
                                <th>Package:</th>
                                <td>@Model.pkg</td>

                            </tr>
                        </table>

                                
                          </div>
                </div>
            </div>
        </div>
    </section>
    <!-- /.content -->
    @Html.Partial("_footer");
    <script>
        document.getElementById("lx").className = "active";
    </script>
</html>
